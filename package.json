{
	"name": "microgravity",
	"version": "1.0.0",
	"description": "",
	"main": "src/server/server.js",
	"scripts": {
		"setup": "node ./scripts/setup.js",
		"start": "node ./src/server/server.js",
		"nodemon": "DOTENV_CONFIG_PATH=.env.dev nodemon -r dotenv/config ./src/server/server.js",
		"serve": "webpack serve --config webpack-dev.config.js",
		"build": "webpack --config webpack-dev.config.js",
		"build:watch": "webpack --watch --config webpack-dev.config.js",
		"build:prod": "webpack --config webpack-prod.config.js",
		"format": "npx prettier --write \"src/**/*.{js,css}\""
	},
	"author": "Nathan Flurry",
	"license": "GPL-3.0-only",
	"dependencies": {
		"@rivet-gg/api": "^23.2.0-rc1",
		"clipboard": "^2.0.1",
		"cors": "^2.8.4",
		"dotenv": "^16.0.1",
		"express": "^4.16.3",
		"file-loader": "^2.0.0",
		"howler": "^2.0.15",
		"html-loader": "^0.5.5",
		"keycode": "^2.2.0",
		"md5": "^2.2.1",
		"msgpack-lite": "^0.1.26",
		"nocache": "^2.0.0",
		"node-fetch": "^2.6.7",
		"quad-node": "^1.0.5",
		"redis": "^2.8.0",
		"regenerator-runtime": "^0.13.9",
		"ssl-root-cas": "^1.3.1",
		"vue": "^2.5.17",
		"vue-i18n": "^8.1.0",
		"ws": "^6.1.2"
	},
	"devDependencies": {
		"@babel/core": "^7.0.1",
		"@babel/preset-env": "^7.0.0",
		"babel-loader": "^8.0.2",
		"babel-plugin-minify-mangle-names": "^0.4.3",
		"babel-preset-minify": "^0.4.3",
		"html-webpack-plugin": "^5.5.0",
		"nodemon": "^1.19.4",
		"safe-area-insets": "^1.3.1",
		"webpack": "^5.73.0",
		"webpack-cli": "^4.10.0",
		"webpack-dev-server": "^4.9.2"
	},
	"prettier": {
		"trailingComma": "none",
		"tabWidth": 4,
		"useTabs": true,
		"singleQuote": true,
		"printWidth": 110,
		"endOfLine": "lf",
		"arrowParens": "avoid",
		"bracketSpacing": true,
		"jsxBracketSameLine": true,
		"jsxSingleQuote": true
	}
